<table>
  <tr>
    <th>Name</th>
    <th>Affiliations</th>
    <th></th>
  </tr>
  <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= affiliations_list_for(member) %></td>
        <td>
          <%= link_to "view", member_path(member.id) %>
          /
          <%= link_to "edit", edit_member_path(member.id) %>
        </td>
      </tr>
  <% end %>
</table>