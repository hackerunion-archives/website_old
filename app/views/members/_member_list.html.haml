%table.table.table-striped
  %thead
    %tr
      %th Name
      %th Affiliations
      %th Skills
      %th Ambassadorness
      %th Email
      %th
  %tbody
    - @members.each do |member|
      %tr
        %td= link_to member.name, member_path(member)
        %td= affiliations_list_for(member)
        %td= skills_list_for(member)
        %td= image_tag 'dolphin.jpg' if member.ambassador?
        %td= member.email
        %td
          = link_to "view", member_path(member.id)
          \/
          = link_to "edit", edit_member_path(member.id)